AArch64 CoFF

(* 0x14000001 = B +1 *)

{
0:X0=0x14000001; 0:X1=P1:L1;
}
 P0             |  P1             ;
   STR W0,[X1]  |    BL L1        ;
                |    MOV X8,X9    ;
                |    BL L1        ;
                |    B Lout       ;
                | L1:             ;
                |    B Lfail      ;
                |    MOV X9,#1    ;
                |    RET          ;
                | Lfail:          ;
                |    MOV X9,#0    ;
                |    RET          ;
                | Lout:           ;
exists 1:X8=1 /\ 1:X9=0
